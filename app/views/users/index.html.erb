<div class="center">
  <h1>All Alpha Bloggers</h1>
</div>
<div class="center">
  <%= will_paginate @users %>
</div>
<% @users.each do |user| %>
<ul class="listing">
  <div class="row center">
    <div class="well col-md-4 col-md-offset-4">
      <li><%= link_to gravatar_for(user), user_path(user) %></li>
      <li><%= user.username %></li>
      <li><small><%= pluralize(user.articles.count, "articles")%></small></li>
    </div>
  </div>
</ul>
<% end %>
<div class="center">
  <%= will_paginate @users %>
</div>